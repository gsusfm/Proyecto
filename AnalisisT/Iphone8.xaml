        <Label Text="Iphone 8 64 GB" VerticalOptions="Center" HorizontalOptions="Center" />
        <Image Source="iphone_8.png" VerticalOptions="Center" HorizontalOptions="Center" WidthRequest="200" HeightRequest="200"/>
        <Label Text="" VerticalOptions="Center" HorizontalOptions="Center" />
        <Label Text="Precio: 699$" VerticalOptions="Center" HorizontalOptions="Center" />
        <Label Text="Email: jesus@gmail.com" VerticalOptions="Center" HorizontalOptions="Center" />
